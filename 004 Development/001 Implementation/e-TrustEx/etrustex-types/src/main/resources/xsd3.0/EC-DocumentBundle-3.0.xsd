<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="ec:schema:xsd:DocumentBundle-3"
			xmlns:eccc="ec:schema:xsd:CommonComponents-3"
			xmlns:sbdh="http://www.unece.org/cefact/namespaces/StandardBusinessDocumentHeader"
			xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			targetNamespace="ec:schema:xsd:DocumentBundle-3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.1">
	<!-- ===== Imports ===== -->
	<xsd:import namespace="ec:schema:xsd:CommonComponents-3" schemaLocation="custom/EC-CommonComponents-3.0.xsd"/>
	<xsd:import namespace="http://www.unece.org/cefact/namespaces/StandardBusinessDocumentHeader" schemaLocation="../xsd/common/sbdh/StandardBusinessDocumentHeader.xsd"/>
	<!-- ===== Root Element ===== -->
	<xsd:element name="DocumentBundle" type="DocumentBundleType"/>
	<xsd:complexType name="DocumentBundleType">
		<xsd:sequence>
			<xsd:element ref="eccc:UUID" minOccurs="0"/>
			<xsd:element name="DocumentIdentification" type="sbdh:DocumentIdentification"/>
			<xsd:element name="ParentDocumentIdentification" type="sbdh:DocumentIdentification"/>
			<xsd:element ref="eccc:Note" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="eccc:OriginatorDocumentReference" minOccurs="0"/>
			<xsd:element ref="eccc:DocumentWrapperReference" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
