<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="ec:services:wsdl:QueryRequest-3" xmlns:req="ec:schema:xsd:QueryRequest-3" xmlns:res="ec:schema:xsd:QueryResponse-3" xmlns:fault="ec:schema:xsd:Fault-3" xmlns:eccc="ec:schema:xsd:CommonComponents-3" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" name="QueryRequest" targetNamespace="ec:services:wsdl:QueryRequest-3">
	<wsdl:types>
		<xsd:schema targetNamespace="ec:services:wsdl:QueryRequest-3" elementFormDefault="qualified">
			<xsd:import namespace="ec:schema:xsd:QueryRequest-3" schemaLocation="../xsd3.0/EC-QueryRequest-3.0.xsd"/>
			<xsd:import namespace="ec:schema:xsd:QueryResponse-3" schemaLocation="../xsd3.0/EC-QueryResponse-3.0.xsd"/>
			<xsd:import namespace="ec:schema:xsd:Fault-3" schemaLocation="../xsd3.0/EC-Fault-3.0.xsd"/>
			<xsd:import namespace="ec:schema:xsd:CommonComponents-3" schemaLocation="../xsd3.0/custom/EC-CommonComponents-3.0.xsd"/>
			
			<xsd:element name="SubmitQueryRequestRequest">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="QueryRequest" type="req:QueryRequestType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SubmitQueryRequestResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="QueryResponse" type="res:QueryResponseType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Header" type="eccc:HeaderType"/>
			<xsd:element name="Fault" type="fault:FaultType"/>
		</xsd:schema>
	</wsdl:types>
	<message name="QueryRequestRequest">
		<part name="QueryRequestRequest" element="tns:SubmitQueryRequestRequest"/>
		<part name="Header" element="tns:Header"/>
	</message>
	<message name="QueryRequestResponse">
		<part name="QueryRequestResponse" element="tns:SubmitQueryRequestResponse"/>
		<part name="Header" element="tns:Header"/>
	</message>
	<message name="FaultResponse">
		<part name="FaultResponse" element="tns:Fault"/>
	</message>
	<portType name="QueryRequestPortType">
		<operation name="submitQueryRequest">
			<input message="tns:QueryRequestRequest"/>
			<output message="tns:QueryRequestResponse"/>
			<fault name="FaultResponse" message="tns:FaultResponse"/>
		</operation>
	</portType>
	<binding name="QueryRequestBinding" type="tns:QueryRequestPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="submitQueryRequest">
			<soap:operation style="document"/>
			<input>
				<soap:body parts="QueryRequestRequest" use="literal"/>
				<soap:header message="tns:QueryRequestRequest" part="Header" use="literal">
					<soap:headerfault message="tns:FaultResponse" part="FaultResponse" use="literal"/>
				</soap:header>
			</input>
			<output>
				<soap:body parts="QueryRequestResponse" use="literal"/>
				<soap:header message="tns:QueryRequestResponse" part="Header" use="literal">
					<soap:headerfault message="tns:FaultResponse" part="FaultResponse" use="literal"/>
				</soap:header>
			</output>
			<fault name="FaultResponse">
				<soap:fault name="FaultResponse" use="literal"/>
			</fault>
		</operation>
	</binding>
	<service name="QueryRequestService">
		<port name="QueryRequestPort" binding="tns:QueryRequestBinding">
			<soap:address location="${etrustex.root.location}/services"/>
		</port>
	</service>
</definitions>
