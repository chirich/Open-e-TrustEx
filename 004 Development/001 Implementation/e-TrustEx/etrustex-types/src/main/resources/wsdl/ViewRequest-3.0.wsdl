<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="ec:services:wsdl:ViewRequest-3" xmlns:req="ec:schema:xsd:ViewRequest-3" xmlns:res="ec:schema:xsd:ViewResponse-3" xmlns:fault="ec:schema:xsd:Fault-3" xmlns:eccc="ec:schema:xsd:CommonComponents-3" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" name="ViewRequest" targetNamespace="ec:services:wsdl:ViewRequest-3">
	<wsdl:types>
		<xsd:schema targetNamespace="ec:services:wsdl:ViewRequest-3" elementFormDefault="qualified">
			<xsd:import namespace="ec:schema:xsd:ViewRequest-3" schemaLocation="../xsd3.0/EC-ViewRequest-3.0.xsd"/>
			<xsd:import namespace="ec:schema:xsd:ViewResponse-3" schemaLocation="../xsd3.0/EC-ViewResponse-3.0.xsd"/>
			<xsd:import namespace="ec:schema:xsd:Fault-3" schemaLocation="../xsd3.0/EC-Fault-3.0.xsd"/>
			<xsd:import namespace="ec:schema:xsd:CommonComponents-3" schemaLocation="../xsd3.0/custom/EC-CommonComponents-3.0.xsd"/>

			<xsd:element name="SubmitViewRequestRequest">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ViewRequest" type="req:ViewRequestType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SubmitViewRequestResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ViewResponse" type="res:ViewResponseType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Header" type="eccc:HeaderType"/>
			<xsd:element name="Fault" type="fault:FaultType"/>
		</xsd:schema>
	</wsdl:types>
	<message name="viewRequestRequest">
		<part name="ViewRequestRequest" element="tns:SubmitViewRequestRequest"/>
		<part name="Header" element="tns:Header"/>
	</message>
	<message name="viewRequestResponse">
		<part name="ViewRequestResponse" element="tns:SubmitViewRequestResponse"/>
		<part name="Header" element="tns:Header"/>
	</message>
	<message name="FaultResponse">
		<part name="FaultResponse" element="tns:Fault"/>
	</message>
	<portType name="viewRequestPortType">
		<operation name="submitViewRequest">
			<input message="tns:viewRequestRequest"/>
			<output message="tns:viewRequestResponse"/>
			<fault name="FaultResponse" message="tns:FaultResponse"/>
		</operation>
	</portType>
	<binding name="viewRequestBinding" type="tns:viewRequestPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="submitViewRequest">
			<input>
				<soap:body parts="ViewRequestRequest" use="literal"/>
				<soap:header message="tns:viewRequestRequest" part="Header" use="literal">
					<soap:headerfault message="tns:FaultResponse" part="FaultResponse" use="literal"/>
				</soap:header>
			</input>
			<output>
				<soap:body parts="ViewRequestResponse" use="literal"/>
				<soap:header message="tns:viewRequestResponse" part="Header" use="literal">
					<soap:headerfault message="tns:FaultResponse" part="FaultResponse" use="literal"/>
				</soap:header>
			</output>
			<fault name="FaultResponse">
				<soap:fault name="FaultResponse" use="literal"/>
			</fault>
		</operation>
	</binding>
	<service name="viewRequestService">
		<port name="viewRequestPort" binding="tns:viewRequestBinding">
			<soap:address location="${etrustex.root.location}/services"/>
		</port>
	</service>
</definitions>
